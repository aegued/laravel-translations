{
    "name": "aegued/laravel-translations",
    "description": "Create translations of the fields of a Model and save them in the database",
    "license": "MIT",
    "authors": [
        {
            "name": "Alfredo",
            "email": "egued89@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php" : ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Aegued\\LaravelTranslations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aegued\\LaravelTranslations\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "Aegued\\LaravelTranslations\\LaravelTranslationsServiceProvider"
          ]
        }
      }
}
